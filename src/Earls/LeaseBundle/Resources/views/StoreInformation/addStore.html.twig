{% extends "EarlsLeaseBundle::layout.html.twig" %}
{% block body %}
    <div class="row">
    <div class="col-md-12">
    <div class="box border" id="formWizard">
    <div class="box-title">
        <h4>
            ADD A STORE <span class="stepHeader">Step 1 of 3</span>
        </h4>
    </div>
    <div class="box-body form">

    {{ form_start(addStoreForm, {
    'attr': {
    'class': 'form-horizontal',
    'id':'wizForm',
    'action': '#',
    }
    }) }}
    <div class="wizard-form">
    <div class="wizard-content">
    <ul class="nav nav-pills nav-justified steps">
        <li>
            <a href="#store" data-toggle="tab" class="wiz-step">
                <span class="step-number">1</span>
                <span class="step-name">CREATE A STORE</span>
            </a>
        </li>
        <li>
            <a href="#lease" data-toggle="tab" class="wiz-step">
                <span class="step-number">2</span>
                <span class="step-name">ADD A LEASE</span>
            </a>
        </li>
        <li>
            <a href="#area" data-toggle="tab" class="wiz-step">
                <span class="step-number">3</span>
                <span class="step-name">ADD THE AREAS</span>
            </a>
        </li>
    </ul>
    <div id="bar" class="progress progress-striped progress-sm active" role="progressbar">
        <div class="progress-bar progress-bar-warning"></div>
    </div>
    <div class="tab-content">
    <div id="warning"></div>


