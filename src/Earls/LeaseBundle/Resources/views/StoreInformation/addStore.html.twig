        <div class="box border blue">
            <div class="box-title">
                <h4>Restaurant Info</h4>
            </div>
            <div class="box-body big">

                <div class="form-group">
                    <label class="col-sm-3 control-label">Store File No.</label>

                    <div class="col-sm-9">

                        {{ form_widget(addStoreForm.storeInfoForm.restaurantInfo.storefilenumber, {
                        'attr': {'class': 'form-control'}
                        }) }}

                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">Store Nick Name</label>

                    <div class="col-sm-9">

                        {{ form_widget(addStoreForm.storeInfoForm.restaurantInfo.storenickname, {'label': 'Store Nick Name',
