<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Earls\CorporateBundle\Entity\Landlordspropertymanagers" table="landlordspropertymanagers">
    <id name="landlordpropertymanid" type="smallint" column="landlordPropertyManID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true"/>
    <field name="address" type="string" column="address" length="45" nullable="true"/>
    <field name="postalzip" type="string" column="postalZip" length="45" nullable="true"/>
    <field name="attention" type="string" column="attention" length="255" nullable="true"/>
    <field name="phone" type="string" column="phone" length="45" nullable="true"/>
    <field name="fax" type="string" column="fax" length="45" nullable="true"/>
    <many-to-one field="city" target-entity="Northamericancities">
      <join-columns>
        <join-column name="city" referenced-column-name="northAmericanCityID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="provincestateid" target-entity="Provincestate">
      <join-columns>
        <join-column name="provinceStateID" referenced-column-name="provinceStateID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
